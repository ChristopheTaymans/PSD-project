		"embeddedBy": "../",
		"i18n": "i18n/i18n.properties",
		"title": "{{compTitle}}",
		"description": "{{compDescription}}",
		"resources": "resources.json",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
